{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "cherry MX3.0s 右下角 超键 1、2 快捷键增强：截图，粘贴",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "1",
                                    "modifiers": {
                                        "mandatory": ["control", "option", "shift"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "9",
                                        "modifiers": ["control", "shift"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "2",
                                    "modifiers": {
                                        "mandatory": ["control", "option", "shift"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "v",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Chrome: Option + Left/Right Arrow to switch tabs",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.google\\.Chrome$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "left_arrow",
                                    "modifiers": {
                                        "mandatory": ["option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "tab",
                                        "modifiers": ["control", "shift"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.google\\.Chrome$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "right_arrow",
                                    "modifiers": {
                                        "mandatory": ["option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "tab",
                                        "modifiers": ["control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "修改 Home End 为正常的跳转行首行尾",
                        "manipulators": [
                            {
                                "description": "修改 Home 为正常的跳转行首 Command+Left Arrow",
                                "from": { "key_code": "home" },
                                "to": [
                                    {
                                        "key_code": "left_arrow",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "修改 End 为正常的跳转行尾 Command+Right Arrow",
                                "from": { "key_code": "end" },
                                "to": [
                                    {
                                        "key_code": "right_arrow",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "删除一行的快捷方式 cmd+shift+k delete line",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "k",
                                    "modifiers": { "mandatory": ["left_command", "left_shift"] }
                                },
                                "to": [
                                    {
                                        "key_code": "a",
                                        "modifiers": ["left_control"],
                                        "repeat": false
                                    },
                                    {
                                        "key_code": "k",
                                        "modifiers": ["left_control"],
                                        "repeat": false
                                    },
                                    {
                                        "key_code": "delete_or_backspace",
                                        "repeat": false
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "双击退出 left cmd + double-tap q → close application",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "command-q",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "to": [
                                    {
                                        "key_code": "q",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "command-q",
                                            "value": 1
                                        }
                                    }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "command-q",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
                                        {
                                            "set_variable": {
                                                "name": "command-q",
                                                "value": 0
                                            }
                                        }
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "在 mac 上使用普通 windows 键盘: Swap left_command with left_option",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_option" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "left_option",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_command" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "在 mac 上使用普通 windows 键盘: Swap right_command with right_option",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "right_option" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_option",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "right_command" }],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
